{
  "name": "gau-monorepo",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "bun@1.2.18",
  "scripts": {
    "build": "bun run build.ts",
    "dev": "bun run dev.ts",
    "lint": "eslint . --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "release": "bumpp packages/gau/package.json && bun run build && npm publish packages/gau --access public"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@types/bun": "latest",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.1",
    "svelte": "^5.0.0-next.201",
    "unbuild": "^3.5.0",
    "vitest": "^3.2.4"
  },
  "workspaces": [
    "packages/*"
  ]
}
