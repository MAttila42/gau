{
  "name": "gau-monorepo",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "bun@1.2.20",
  "scripts": {
    "build": "bun --bun --cwd packages/gau tsup",
    "dev": "bun --bun --cwd packages/gau tsup --watch",
    "lint": "eslint . --fix",
    "lint:test": "eslint coverage.json --fix",
    "test": "vitest && bun run lint:test",
    "test:ui": "vitest --ui",
    "update:deps": "taze latest -wr",
    "release": "bumpp packages/gau/package.json packages/gau/jsr.json && bun run build && npm publish --workspace @rttnd/gau --access public"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@types/bun": "^1.2.20",
    "@typescript/native-preview": "^7.0.0-dev.20250819.1",
    "@unocss/eslint-plugin": "^66.4.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-svelte": "^3.11.0",
    "prettier-plugin-astro": "^0.14.1",
    "svelte": "^5.38.2",
    "taze": "^19.1.0",
    "terser": "^5.43.1",
    "tsup": "^8.5.0",
    "unbuild": "^3.6.1",
    "vitest": "^3.2.4",
    "wrangler": "^4.31.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "trustedDependencies": [
    "@parcel/watcher",
    "better-sqlite3",
    "esbuild",
    "sharp",
    "workerd"
  ],
  "resolutions": {
    "vite": "^7.1.3"
  }
}
