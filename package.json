{
  "name": "gau-monorepo",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "bun@1.2.19",
  "scripts": {
    "build": "bun --bun --cwd packages/gau tsup",
    "dev": "bun --bun --cwd packages/gau tsup --watch",
    "lint": "eslint . --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "release": "bumpp packages/gau/package.json && bun run build && npm publish --workspace @rttnd/gau --access public"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@types/bun": "^1.2.18",
    "@typescript/native-preview": "^7.0.0-dev.20250715.1",
    "@unocss/eslint-plugin": "^66.3.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.31.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-svelte": "^3.10.1",
    "prettier-plugin-astro": "^0.14.1",
    "svelte": "^5.35.6",
    "tsup": "^8.5.0",
    "unbuild": "^3.5.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.24.3"
  },
  "workspaces": [
    "packages/*"
  ]
}
